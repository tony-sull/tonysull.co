---
import Head from '@components/Head.astro'
import type { Props as HeadProps } from '@components/Head.astro'

export interface Props extends HeadProps {}

const { ...head } = Astro.props
---

<html lang="en" data-chisel-theme="light">
  <head>
    <Head {...head} />
  </head>

  <body>
    <main id="main">
      <slot />
    </main>
  </body>
</html>
